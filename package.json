{
  "name": "metadata-github-to-mongo",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "Transfers GitHub-API-Data to mongoDB",
  "main": "src/index.ts",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "scripts": {
    "build": "tsc",
    "start": "tsc && probot run ./lib/index.js",
    "test": "tsc && jest"
  },
  "author": "Sven Peschau",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.0",
    "mongodb": "^4.4.1",
    "nock": "^13.2.4",
    "probot": "^11.0.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.14.19",
    "babel-jest": "^27.5.1",
    "jest": "^27.5.1",
    "smee-client": "^1.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">= 10.13.0"
  }
}
